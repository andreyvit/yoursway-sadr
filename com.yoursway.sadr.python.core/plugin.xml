<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.dltk.core.typeEvaluators">
      <evaluator
            evaluator="com.yoursway.sadr.python.DltkTypeInferencingAdapter"
            nature="org.eclipse.dltk.ruby.core.nature">
      </evaluator>
   </extension>
   <extension
         point="org.eclipse.dltk.core.completionEngine">
      <completionEngine
            class="com.yoursway.sadr.python.completion.PythonCompletionEngine"
            nature="org.eclipse.dltk.python.core.nature"
            priority="0">
      </completionEngine>
   </extension>

</plugin>
